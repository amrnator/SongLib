package Model;
import java.io.*;

public class FileRead {
	
	private static BufferedReader inputFile;
	
	//reads input from a text file and returns a 
	public static SongList createSongList (String fileName) throws IOException{
		SongList list = new SongList();
		try{
			//read from file
			FileReader x = new FileReader(fileName);
			//add buffer for safety reasons
			inputFile = new BufferedReader(x);
			
		}catch(FileNotFoundException ex){
			System.out.println("YOU FUCKED UP");
		}
		System.out.println(inputFile.readLine());
		return list;
	} 
}
